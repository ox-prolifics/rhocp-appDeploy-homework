#if [ "$opt" = "y" ]; then `./setup_jenkins.sh 05f3 https://github.com/ox-prolifics/rhocp-appDeploy-homework.git https://master.na311.openshift.opentlc.com 2>&1 |  tee setJenkins.log`; cat setJenkins.log; fi
